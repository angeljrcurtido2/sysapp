{
  "expo": {
    "name": "KontrolX",
    "slug": "sysapp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "scheme": "kontrolx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.angeljrcurtido.kontrolx"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#38bdf8",
        "foregroundImage": "./assets/images/icon2.png",
        "monochromeImage": "./assets/images/icon2.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.angeljrcurtido.kontrolx",
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#38bdf8",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#0284c7"
          }
        }
      ],
      "./plugins/withAndroidX.js",
      "./plugins/withVoicePermissions.js"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8df20f1e-9199-4743-909c-55614aa37243"
      }
    }
  }
}
